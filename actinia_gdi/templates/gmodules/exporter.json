{
    "export": [
        {
            "name": "format",
            "description": "The format of the output file",
            "optional": false,
            "schema": {
                "enum": [
                    "GTiff",
                    "GPKG",
                    "SQLite",
                    "GML",
                    "GeoJSON",
                    "ESRI_Shapefile",
                    "CSV",
                    "TXT",
                    "PostgreSQL"
                ],
                "type": "string"
            }
        },
        {
            "name": "type",
            "description": "The type of the output",
            "optional": false,
            "schema": {
                "enum": [
                    "raster",
                    "vector",
                    "file"
                ],
                "type": "string"
            }
        },
        {
            "name": "dbstring",
            "description": "The database string to be used to connect to a PostgreSQL database for vector export.",
            "optional": true,
            "schema": {
                "type": "string"
            }
        },
        {
            "name": "output_layer",
            "description": "Name for output PostgreSQL layer.",
            "optional": true,
            "schema": {
                "type": "string"
            }
        }
    ]
}
